{
    "name": "slaxweb/bootstrap",
    "description": "Bootstrap for SlaxWeb Framework",
    "license": "MIT",
    "version": "0.3.1",
    "authors": [
        {
            "name": "Tomaz Lovrec",
            "email": "tomaz.lovrec@gmail.com"
        }
    ],
    "require": {
        "php":  ">=7.0.0",
        "slaxweb/router": "0.4.*@dev",
        "slaxweb/hooks": "0.5.*@dev",
        "slaxweb/logger": "0.3.*@dev",
        "slaxweb/config": "0.2.*@dev",
        "pimple/pimple": "3.0.2"
    },
    "require-dev": {
        "codeception/codeception": "dev-master",
        "codeception/specify": "*",
        "behat/gherkin": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "SlaxWeb\\Bootstrap\\": "src",
            "SlaxWeb\\Bootstrap\\Tests\\Unit\\": "tests/unit",
            "SlaxWeb\\Bootstrap\\Tests\\Helper\\": "tests/_support/Helper"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        }
    },
    "minimum-stability": "dev"
}
